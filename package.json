{
  "name": "@ramp/api-crypto-address-validator",
  "description": "Wallet address validator for Bitcoin and other Altcoins.",
  "main": "dist/ramp-crypto-address-validator.umd.js",
  "module": "dist/ramp-crypto-address-validator.es5.js",
  "typings": "dist/types/ramp-crypto-address-validator.d.ts",
  "version": "1.13.1",
  "author": "Swyftx",
  "publishConfig": {
    "access": "public"
  },
  "contributors": [
    {
      "name": "Tomek Kolasa",
      "email": "tomek@kolasa.me"
    }
  ],
  "homepage": "https://github.com/Swyftx/crypto-address-validator",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Swyftx/crypto-address-validator.git"
  },
  "scripts": {
    "minify": "uglifyjs -c -m -o dist/wallet-address-validator.min.js -- dist/wallet-address-validator.js",
    "test:node": "mocha -r ts-node/register test/*.ts",
    "test:browser": "yarn run build:ts && karma start",
    "test": "yarn run build && npm run test:node && npm run test:browser",
    "start": "npm run build && npm test",
    "lint": "eslint --ext .ts .",
    "lint:fix": "yarn lint --fix",
    "generateReadme": "node debug/generateReadme.js && git add README.md dist/wallet-address-validator.min.js.gz package.json",
    "createMin": "yarn run bundle && git add dist/wallet-address-validator.js dist/wallet-address-validator.min.js",
    "preversion": "yarn run createMin && yarn run generateReadme",
    "postversion": "git push origin HEAD --follow-tags && hub pull-request -o -b dev -m v%npm_package_version%",
    "build:ts": "tsc --module commonjs",
    "build:rollup": "rollup -c rollup.config.ts",
    "build": "yarn run build:ts && yarn run build:rollup"
  },
  "dependencies": {
    "@iota/validators": "^1.0.0-beta.6",
    "base-x": "^3.0.4",
    "bchaddrjs-slp": "^0.2.8",
    "bech32": "^2.0.0",
    "bignumber.js": "^9.0.2",
    "blake-hash": "^2.0.0",
    "blakejs": "^1.1.1",
    "bs58": "^4.0.1",
    "c32check": "^1.0.1",
    "cbor": "^8.1.0",
    "js-sha512": "^0.8.0",
    "jssha": "3.2.0",
    "keccak": "^3.0.2",
    "rfc4648": "^1.3.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/bluebird": "^3.5.36",
    "@types/bs58": "^4.0.1",
    "@types/chai": "^4.3.0",
    "@types/expect": "^24.3.0",
    "@types/keccak": "^3.0.1",
    "@types/mocha": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "browserify": "^17.0.0",
    "buffer": "^6.0.3",
    "chai": "^4.3.4",
    "crypto-browserify": "^3.12.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-brackets": "^0.1.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unicorn": "^35.0.0",
    "eslint-plugin-unused-imports": "^1.0.1",
    "karma": "^6.3.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.1",
    "karma-typescript": "^5.5.2",
    "karma-webpack": "^5.0.0",
    "mocha": "^9.1.3",
    "node-gzip": "^1.1.2",
    "prettier": "^2.5.1",
    "prettysize": "^2.0.0",
    "puppeteer": "^13.0.1",
    "rollup": "^2.63.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.31.1",
    "standard": "^16.0.4",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "uglify-es": "^3.3.10",
    "watchify": "^4.0.0",
    "webpack": "^5.65.0"
  },
  "standard": {
    "ignore": [
      "dist/"
    ]
  }
}
